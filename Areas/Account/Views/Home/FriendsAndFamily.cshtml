@model FriendsAndFamilyModel

<h1>F&F</h1>
<div class="container py-5">
    <div class="row">
        <div class="col-md-12"></div>
    </div>
</div>
<table>
    <thead>
        <tr>
            <th>Email</th>
            <th>Name</th>
            <th>Mobile</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var account in Model.AccountModels)
        {
            <tr>
                <td>@account.EMail</td>
                <td>@account.Name</td>
                <td>@account.MobileTelephone</td>
                <td>
                    view | edit | delete
                </td>
            </tr>
        }
    </tbody>
</table>